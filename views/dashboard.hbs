{{#if deliveryData}}
  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Client</th>
        <th>Description</th>
      </tr>
    </thead>
    <tbody>
      {{#each deliveryData}}
        <tr>
          <td>{{this.date}}</td>
          <td>{{this.client}}</td>
          <td>{{this.description}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
{{else}}
  <p>No delivery data available</p>
{{/if}}



